doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/custom.css')
  body
    #container(style='min-height: 100%;')
      .row
        nav.navbar.navbar-default.navbar-fixed-top.navbar-inverse
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='#collapse')
              span.sr-only Toggle navigation
              span.glyphicon.glyphicon-arrow-down
              |                   MENU
          #collapse.collapse.navbar-collapse
            ul.nav.navbar-nav
              li
                a.active(href='/') Start
              li
                a(href='sessionlist') Current Sessions
              li
                a(href='#') About
      block content
      block foot
        footer.music-footer
          .row
            div
              img(src='images/MusicFooterSmTransparent.png')
        script(src='http://code.jquery.com/jquery-latest.min.js')
        script(src='javascripts/bootstrap.min.js')