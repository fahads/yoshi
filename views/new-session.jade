extends layout

block content
  h1= title
  p Welcome to #{sessionname}
  .row
    // LEFT COLUMN
    .col-lg-3.col-md-3.col-sm-3
      h3 Session Information
      div
        h4= sessionname
        h4 Instrument
        #sessinstrument= sessinstrument
      br
      // CENTER COLUMN
    .col-lg-3.col-md-3.col-sm-3
      .session-logo
        .this-session-logo
          img(src='images/MusicBirdLrgLightGold.png', alt='Musicador Logo', width='200')
      // RIGHT COLUMN
    .col-lg-6.col-md-6.col-sm-6
      h4 Participants
      ul
        each user in sessusers
          li= user[0] + ' is playing the ' + user[1]
  .row
    div(style='text-align: center;')
      // img(src='images/MusicKeyboard.png', alt='Keyboard')
      include ../public/piano.html